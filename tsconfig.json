{
  "compilerOptions": {
    "outDir": "dist",
    "skipLibCheck": true,
    "declaration": true,
    "baseUrl": ".",
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "allowUnreachableCode": true,
    "lib": [
      "es2015",
      "es2016",
      "es2017"
    ]
  },
  "skipLibCheck": true,
  "compileOnSave": false,
  "exclude": [
    "public",
    "node_modules"
  ],
  "include":[
    "lib"
  ]
}
